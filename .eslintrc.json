{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-tabs":"off",
        "indent":"off",
        "implicit-arrow-linebreak":"off",
        "quotes":"off",
        "no-console":"off",
        "import/no-unresolved":"off",
        "import/no-named-as-default":"off",
        "import/no-named-as-default-member":"off",
        "operator-linebreak":"off"
    }
}
