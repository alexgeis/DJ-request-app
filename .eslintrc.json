{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
            "no-console": "off",
            "no-tabs": "off",
            "indent": "off",
            "no-restricted-syntax": "off",
            "guard-for-in": "off"
    }
}
